{
  "name": "frontend-state",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "start-storybook --quiet --ci -p 9001 -c .storybook",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:ts'",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "start": "webpack-dev-server --host 0.0.0.0 --port 9001 --hide-modules",
    "getGqlSchema": "get-graphql-schema -h \"authorization=$AUTH_HEADER\" $GQL_ENDPOINT > schema.graphql",
    "mst-gql:clean": "rimraf src/mst-gql/generated",
    "mst-gql:generate": "mst-gql schema.graphql",
    "gqless:generate": "gqless generate ./src/gqless/generated -t -u $GQL_ENDPOINT --header=\"authorization:$AUTH_HEADER\"",
    "env": "dotenv -c dev --",
    "build:depGraph": "depcruise --include-only '^src' --output-type dot client | dot -T svg > dependencygraph.svg",
    "build:analyze": "yarn -s webpack -p --profile --json > build/stats.json && yarn -s webpack-bundle-analyzer ./build/stats.json ./build"
  },
  "dependencies": {
    "@gqless/logger": "^1.0.0-alpha.28",
    "@gqless/react": "^1.0.0-alpha.31",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.23",
    "emotion": "^10.0.27",
    "gqless": "^1.0.0-alpha.31",
    "history": "^5.0.0",
    "mobx": "5",
    "mobx-react-lite": "2",
    "mobx-state-tree": "^3.17.2",
    "mst-gql": "^0.13.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@gqless/cli": "^1.0.0-alpha.33",
    "@storybook/react": "6.1.0-alpha.29",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "~7.11.0",
    "eslint-config-prettier": "~6.13.0",
    "eslint-config-standard": "~14.1.1",
    "eslint-config-standard-typescript-prettier": "~3.2.0",
    "eslint-import-resolver-node": "~0.3.4",
    "eslint-plugin-import": "~2.22",
    "eslint-plugin-node": "~11.0",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-prettier": "~3.1",
    "eslint-plugin-promise": "~4.2",
    "eslint-plugin-react": "~7.21.4",
    "eslint-plugin-react-hooks": "~4.1.2",
    "eslint-plugin-standard": "~4.0.1",
    "get-graphql-schema": "^2.1.2",
    "typescript": "^4.0.3",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  }
}
