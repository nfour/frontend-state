{
  "name": "frontend-state",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "start-storybook --quiet --ci -p 9001 -c .storybook",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:ts'",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "start": "webpack-dev-server --host 0.0.0.0 --port 9001 --hide-modules",
    "build": "yarn build:clean && yarn build:prodEnvFile; webpack -p --env.production --color --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build/*",
    "build:depGraph": "depcruise --include-only '^src' --output-type dot client | dot -T svg > dependencygraph.svg",
    "build:analyze": "yarn -s webpack -p --profile --json > build/stats.json && yarn -s webpack-bundle-analyzer ./build/stats.json ./build"
  },
  "dependencies": {
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.23",
    "emotion": "^10.0.27",
    "history": "^5.0.0",
    "mobx": "5",
    "mobx-react-lite": "^3.0.1",
    "mobx-state-tree": "^3.17.2",
    "mst-gql": "^0.13.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@storybook/react": "^6.0.27",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-typescript-prettier": "~2.2.3",
    "eslint-import-resolver-webpack": "~0.12.1",
    "eslint-plugin-babel": "~5.3.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "~6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "~7.20.0",
    "eslint-plugin-react-hooks": "~4.0.4",
    "eslint-plugin-standard": "^4.0.1",
    "typescript": "^4.0.3",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  }
}
