{
  "name": "frontend-state",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "start-storybook --quiet --ci -p 9001 -c .storybook",
    "lint": "concurrently 'yarn lint:eslint' 'yarn lint:ts'",
    "lint:eslint": "eslint .",
    "lint:ts": "tsc --noEmit --incremental false --tsBuildInfoFile null",
    "start": "webpack-dev-server --host 0.0.0.0 --port 9001 --hide-modules",
    "getGqlSchema": "get-graphql-schema -h \"authorization=$AUTH_HEADER\" $GQL_ENDPOINT > schema.graphql",
    "mst-gql:clean": "rimraf src/mst-gql/generated",
    "mst-gql:generate": "mst-gql schema.graphql",
    "gqless:generate": "gqless generate ./src/gqless/generated -t -u $GQL_ENDPOINT --header=\"authorization:$AUTH_HEADER\"",
    "env": "dotenv -c dev --",
    "build:depGraph": "depcruise --include-only '^src' --output-type dot client | dot -T svg > dependencygraph.svg",
    "build:analyze": "yarn -s webpack -p --profile --json > build/stats.json && yarn -s webpack-bundle-analyzer ./build/stats.json ./build"
  },
  "dependencies": {
    "@gqless/logger": "^1.0.0-alpha.28",
    "@gqless/react": "^1.0.0-alpha.31",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "@types/webpack": "^4.41.23",
    "emotion": "^10.0.27",
    "fp-ts": "^2.8.6",
    "gqless": "^1.0.0-alpha.31",
    "history": "^5.0.0",
    "io-ts": "^2.2.13",
    "lodash": "^4.17.20",
    "mobx": "6",
    "mobx-react-lite": "3",
    "mobx-state-tree": "4",
    "mst-gql": "^0.13.0",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "tiny-mst-router": "^2.0.0",
    "ts-toolbelt": "^8.0.7",
    "type-fest": "^0.19.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@gqless/cli": "^1.0.0-alpha.33",
    "@storybook/react": "6.1.0-alpha.29",
    "@typescript-eslint/eslint-plugin": "~4.8.1",
    "@typescript-eslint/parser": "~4.8.1",
    "babel-loader": "~8.2.1",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "~7.14.0",
    "eslint-config-prettier": "6.15",
    "eslint-config-standard": "16.0",
    "eslint-config-standard-typescript-prettier": "3.2",
    "eslint-import-resolver-node": "0.3",
    "eslint-plugin-import": "2.22",
    "eslint-plugin-node": "11.1",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "~7.21.5",
    "eslint-plugin-react-hooks": "~4.2",
    "eslint-plugin-standard": "~4.1",
    "get-graphql-schema": "^2.1.2",
    "typescript": "^4.1",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  }
}
